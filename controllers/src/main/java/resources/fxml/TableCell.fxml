<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<fx:root fx:id="tableCell" type="VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="articleTitle" maxHeight="1.7976931348623157E308" minHeight="-Infinity" minWidth="0.0" text="" textFill="#f3f3f3" wrapText="true" VBox.vgrow="ALWAYS">
         <font>
            <Font size="18.0" />
         </font>
         <padding>
            <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER_LEFT" fillHeight="false" spacing="10.0" VBox.vgrow="NEVER">
         <children>
            <Label fx:id="createdBy" textFill="#f3f3f3">
               <font>
                  <Font size="13.0" />
               </font>
            </Label>
            <Separator orientation="VERTICAL" HBox.hgrow="NEVER" />
            <Hyperlink fx:id="url" textFill="#f3f3f3" underline="true">
               <font>
                  <Font size="13.0" />
               </font>
            </Hyperlink>
            <Separator orientation="VERTICAL" HBox.hgrow="NEVER" />
            <Label fx:id="dateCreated" textFill="#f3f3f3">
               <font>
                  <Font size="13.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
         </padding>
      </HBox>
   </children>
</fx:root>
